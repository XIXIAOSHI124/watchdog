{
  "name": "watchdog",
  "version": "0.0.1",
  "description": "Service for watching IP Address to update DNS configuration",
  "main": "/src/index.js",
  "scripts": {
    "start": "npm-run-all prod",
    "serve": "node ./dist/server.js",
    "build": "npm-run-all clean compile",
    "compile": "babel ./src -d dist",
    "dev": "cross-env NODE_ENV=development npm-run-all build serve",
    "prod": "cross-env NODE_ENV=production npm-run-all build serve",
    "watch:dev": "nodemon",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XIXIAOSHI124/watchdog.git"
  },
  "keywords": [
    "REST",
    "Service"
  ],
  "author": "XIXIAOSHI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XIXIAOSHI124/watchdog/issues"
  },
  "homepage": "https://github.com/XIXIAOSHI124/watchdog#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "axios": "^0.21.1",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-router": "^10.0.0",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
